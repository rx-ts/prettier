{
  "name": "@rxts/prettier",
  "version": "0.0.0",
  "description": "Opinionated but Incredible Prettier plugins.",
  "repository": "git@github.com:rx-ts/prettier.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build && run-p build:*",
    "build:r": "r -g \"{'prettier/parser-babylon':'prettierPlugins.babylon','prettier-plugin-pkg':'prettierPlugins.pkg','prettier-plugin-sh':'prettierPlugins.sh'}\" -x mvdan-sh -p",
    "build:ts": "tsc -b",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --ext js,md,ts -f friendly",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "type-coverage": "type-coverage --cache --detail --ignore-files *.d.ts --strict"
  },
  "devDependencies": {
    "@1stg/babel-preset": "^0.7.4",
    "@1stg/commitlint-config": "^0.1.0",
    "@1stg/eslint-config": "^0.12.20",
    "@1stg/husky-config": "^0.3.1",
    "@1stg/lint-staged": "^0.8.6",
    "@1stg/prettier-config": "^0.4.2",
    "@1stg/remark-config": "^0.2.2",
    "@1stg/tsconfig": "^0.6.0",
    "@1stg/tslint-config": "^0.4.3",
    "@babel/core": "^7.6.2",
    "@babel/types": "^7.6.1",
    "@commitlint/cli": "^8.2.0",
    "@pkgr/imagemin": "^0.1.2",
    "@pkgr/rollup": "^0.2.4",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.144",
    "@types/prettier": "^1.18.3",
    "eslint": "^6.5.1",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "js-yaml": "^3.13.1",
    "lerna": "^3.16.4",
    "lerna-changelog": "^0.8.2",
    "lint-staged": "^9.4.1",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "rollup": "^1.23.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "type-coverage": "^2.3.0"
  },
  "resolutions": {
    "babel-eslint": "^10.0.3",
    "typescript": "^3.7.0-beta"
  },
  "commitlint": {
    "extends": [
      "@1stg"
    ]
  },
  "eslintIgnore": [
    "lib",
    "packages/sh/src/languages.ts",
    "CHANGELOG.md",
    "!/.*.js"
  ],
  "jest": {
    "collectCoverage": true,
    "moduleNameMapper": {
      "^prettier-plugin-pkg$": "<rootDir>/packages/pkg/src",
      "^prettier-plugin-sh$": "<rootDir>/packages/sh/src"
    },
    "preset": "ts-jest"
  },
  "prettier": "@1stg/prettier-config",
  "renovate": {
    "extends": "@1stg"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
