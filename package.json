{
  "name": "@rxts/prettier",
  "version": "0.0.0",
  "description": "Opinionated but Incredible Prettier plugins.",
  "repository": "git@github.com:rx-ts/prettier.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:r": "r -g \"{'mvdan-sh':'sh','prettier/parser-babel':'prettierPlugins.babel','prettier-plugin-pkg':'prettierPlugins.pkg','prettier-plugin-sh':'prettierPlugins.sh'}\" -x mvdan-sh -p",
    "build:ts": "tsc -b",
    "format": "ts-node scripts/format",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,ts -f friendly",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --incremental false --noEmit",
    "postinstall": "yarn-deduplicate || exit 0",
    "prebuild": "lerna run build",
    "test": "jest",
    "type-coverage": "type-coverage --cache --detail --ignore-files '*.d.ts' --strict --update"
  },
  "devDependencies": {
    "@1stg/lib-config": "^0.5.5",
    "@1stg/tslint-config": "^0.8.5",
    "@babel/types": "^7.11.0",
    "@pkgr/imagemin": "^0.4.5",
    "@types/jest": "^26.0.10",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.162",
    "@types/mvdan-sh": "^0.5.0",
    "@types/prettier": "^2.0.2",
    "core-js": "^3.6.5",
    "https-proxy-agent": "^5.0.0",
    "js-yaml": "^3.14.0",
    "lerna": "^3.22.1",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-pkg": "link:packages/pkg/src",
    "prettier-plugin-sh": "link:packages/sh/src",
    "react": "^16.13.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "type-coverage": "^2.10.0",
    "typescript": "^4.0.2",
    "yarn-deduplicate": "^2.1.1"
  },
  "resolutions": {
    "prettier": "^2.1.0"
  },
  "commitlint": {
    "extends": [
      "@1stg"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "prettier": "@1stg/prettier-config",
  "renovate": {
    "extends": "@1stg"
  },
  "typeCoverage": {
    "atLeast": 99.09
  }
}
